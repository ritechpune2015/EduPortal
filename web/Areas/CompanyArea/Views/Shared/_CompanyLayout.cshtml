            <i class="bi bi-search"></i>
          </a>
        </li><!-- End Search Icon-->

        <li class="nav-item dropdown pe-3">

          <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
            <span class="d-none d-md-block dropdown-toggle ps-2">
             @Context.Session.GetString("CompanyName")
            </span>
          </a><!-- End Profile Iamge Icon -->

          <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
          
            <li>
              <hr class="dropdown-divider">
            </li>

            <li>
              <a class="dropdown-item d-flex align-items-center" asp-action="EditProfile" asp-controller="TrainingCompanyHome">
                <i class="bi bi-person"></i>
                <span>My Profile</span>
              </a>
            </li>
            <li>
              <hr class="dropdown-divider">
            </li>

            <li>
                            <a class="dropdown-item d-flex align-items-center" asp-action="ChangePassword" asp-controller="TrainingCompanyHome">
                <i class="bi bi-gear"></i>
                <span>Change Password</span>
              </a>
            </li>
            <li>
              <hr class="dropdown-divider">
            </li>

           
          

            <li>
                            <a class="dropdown-item d-flex align-items-center" asp-action="SignOut" asp-controller="ManageTrainingCompanies" asp-area="">
                <i class="bi bi-box-arrow-right"></i>
